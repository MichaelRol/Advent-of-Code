{
	"compilerOptions": {
		"target": "es2022",
		"module": "commonjs",
		"noEmit": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": false,
		"forceConsistentCasingInFileNames": true,
		"sourceMap": true,
        "noUnusedLocals": true,
		"allowJs": true
	},
	"include": ["**/*.ts", "**/*.d.ts"]
}